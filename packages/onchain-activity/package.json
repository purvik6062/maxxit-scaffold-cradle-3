{
    "name": "@cradle/onchain-activity",
    "version": "0.1.0",
    "description": "Onchain Activity - fetch wallet transactions by category from Arbitrum",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "viem": "^2.21.0"
    },
    "peerDependencies": {
        "react": "^18.0.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.0",
        "react": "^18.2.0",
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    },
    "files": [
        "dist",
        "src"
    ],
    "keywords": [
        "onchain",
        "activity",
        "transactions",
        "arbitrum",
        "alchemy",
        "web3"
    ],
    "license": "MIT"
}
