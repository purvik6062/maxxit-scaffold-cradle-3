# Environment Variables
# Copy this file to .env and fill in the values

# WalletConnect Cloud project ID (required)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=

# Application name for wallet dialogs
NEXT_PUBLIC_APP_NAME=My DApp

# Maxxit API base URL
MAXXIT_API_BASE_URL=http://localhost:5000

# Network for Ostium trading (arbitrum or arbitrum-sepolia) (required)
NEXT_PUBLIC_OSTIUM_NETWORK=arbitrum

# Alchemy API key for fetching onchain activity (required)
ALCHEMY_API_KEY=

# Network for onchain activity (arbitrum or arbitrum-sepolia) (required)
NEXT_PUBLIC_ONCHAIN_NETWORK=arbitrum

# WalletConnect Cloud project ID for wallet connections (required)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=

# Application name displayed in wallet dialogs
NEXT_PUBLIC_APP_NAME=My DApp